<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://movieshop-jg.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>movieshop-jg.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>movieshop-jg</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$movieshop-jg</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="server=localhost;port=3306;database=MovieWeb;uid=root;password=1234" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="MovieWebAPI.Models.ApplicationDbContext, MovieWebAPI" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="MovieWeb" Order="2" Enabled="False">
          <Destination Path="server=localhost;port=3306;database=MovieWeb;uid=root;password=1234" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="MovieWebDAL.Context.MovieWebContext, MovieWebDAL" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>server=localhost;port=3306;database=MovieWeb;uid=root;password=1234</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)MovieWeb-Web.config Connection String">
      <ParameterValue>server=localhost;port=3306;database=MovieWeb;uid=root;password=1234</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DefaultConnection" />
  </ItemGroup>
</Project>